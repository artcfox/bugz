<?xml version="1.0" ?>
<gfx-xform version="1">
  <input file="sprites.png" type="png" tile-width="8" tile-height="8" />
  <output file="sprites.inc">
    <tiles var-name="mysprites"/>
    <maps pointers-size="8">
      <map var-name="yellow_side" left="0" top="0" width="1" height="1"/>
      <map var-name="yellow_front" left="1" top="0" width="1" height="1"/>
      <map var-name="orange_side" left="2" top="0" width="1" height="1"/>
      <map var-name="orange_front" left="3" top="0" width="1" height="1"/>
      <map var-name="ladybug_side" left="4" top="0" width="1" height="1"/>
      <map var-name="ant_side" left="5" top="0" width="1" height="1"/>
    </maps>
  </output>
</gfx-xform>